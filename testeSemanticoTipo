program exemplo;
	var a, b: integer;
	
	procedure p(x:integer);
		var b, c: integer;

		begin
			b := x + c;
		end;
	
	procedure q;
		var c, d: integer;

		procedure r(y:integer);
			var e: real; f: integer;
			
			begin
				f := y;
				y := 10;
			end;

		begin
			if 2 > 1 then
				c := 2
			else
				c := 1
		end;

	begin
		a := 10;

	end.
